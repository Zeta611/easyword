{
  "name": "easyword.kr",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"bun run build:watch\" \"bun run relay:watch\" \"bun run dev\"",
    "build": "rescript",
    "build:deps": "rescript build -with-deps",
    "build:watch": "rescript build -w",
    "relay": "rescript-relay-compiler",
    "relay:watch": "rescript-relay-compiler --watch",
    "clean": "rescript clean -with-deps",
    "dev": "bunx --bun vite",
    "preview": "vite preview",
    "release": "vite build"
  },
  "keywords": [
    "rescript"
  ],
  "author": "Jay Lee <jaeho.lee@snu.ac.kr>",
  "license": "MIT",
  "packageManager": "bun@1.1.3",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2% in KR",
    "iOS >= 12.5"
  ],
  "dependencies": {
    "@glennsl/rescript-fetch": "^0.2.0",
    "@heroicons/react": "^2.0.13",
    "@rescript/core": "^1.2.0",
    "@rescript/react": "^0.12.1",
    "better-react-mathjax": "^2.0.3",
    "daisyui": "^4.8.0",
    "firebase": "^9.16.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.0.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-relay": "16.0.0",
    "react-social-login-buttons": "^4.1.0",
    "reactfire": "^4.2.2",
    "relay-runtime": "16.1.0",
    "rescript-relay": "3.0.0-rc.4",
    "rescript-webapi": "^0.9.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.8",
    "concurrently": "^5.0.0",
    "lightningcss": "^1.24.1",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^9.5.4",
    "rescript": "^11.0.1",
    "tailwindcss": "^3.1.8",
    "terser": "^5.30.3",
    "vite": "^5.2.7",
    "vite-plugin-legacy-swc": "^1.1.0"
  },
  "trustedDependencies": [
    "@swc/core",
    "core-js",
    "esbuild",
    "protobufjs",
    "rescript",
    "rescript-relay"
  ]
